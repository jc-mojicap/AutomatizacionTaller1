<?xml version="1.0" encoding="UTF-8"?>
<taller1:NoorApp
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:taller1="http://www.example.org/taller1"
    nombreApp="MiLibreriApp"
    colorTexto="Negro"
    colorBoton="Amarillo"
    colorBarra="Verde">
  <entidades
      nombre="Categoria">
    <relacion
        entidad="//@entidades.1"
        esDebil="true"/>
  </entidades>
  <entidades
      nombre="Libro">
    <detalles
        nombre="Autor"
        isPrimary="true"/>
    <detalles
        nombre="publicationYear"
        valor="EDate"
        isPrimary="true"/>
    <detalles
        nombre="Paginas"
        valor="EInteger"/>
    <detalles
        nombre="Calificacion"
        valor="EInteger"/>
  </entidades>
</taller1:NoorApp>
