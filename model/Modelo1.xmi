<?xml version="1.0" encoding="UTF-8"?>
<taller1:NoorApp
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:taller1="http://www.example.org/taller1"
    xsi:schemaLocation="http://www.example.org/taller1 taller1.ecore"
    nombreApp="ContactosApp">
  <entidades
      nombre="Persona">
    <detalles
        nombre="Nombre"/>
    <detalles
        nombre="Telefono"/>
    <detalles
        nombre="Email"/>
    <detalles
        nombre="esActivo"
        valor="EBoolean"
        isPrimary="true"/>
  </entidades>
  <entidades
      nombre="Grupo">
    <detalles
        nombre="Nombre"/>
    <relacion
        entidad="//@entidades.0"/>
  </entidades>
</taller1:NoorApp>
